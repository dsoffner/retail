<%- include layouts/header.ejs %>

        <div class="page-data">
         <div class="data-table">
            <table border="1" cellpadding="7" cellspacing="7">
                <tr>
                    <th width="50px">Id</th>
                    <th>Title</th>
                    <th>Type</th>
                    <th>Description</th>
                    <th>Height</th>
                    <th>Width</th>
                    <th>Price</th>
                    <th>Rating</th>
                </tr>

                <% if(data.length){

                 for(var i = 0;i < data.length;i++) { %>

                <tr>
                    <td><%=(i+1)%></td>
                    <td><%=data[i].title%></td>
                    <td><%=data[i].type%></td>
                    <td><%=data[i].description%></td>
                    <td><%=data[i].height%></td>
                    <td><%=data[i].width%></td>
                    <td><%=data[i].price%></td>
                    <td><%=data[i].rating%></td>
                </tr>
            <% }

             }else{ %>
                 <tr>
                    <td colspan="3">No user</td>
                 </tr>
            <% } %>



            </table>
         </div>
        </div>

<%- include layouts/footer.ejs %>
